{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/ps",
      "destination": "https://itunes.apple.com/jp/app/sdvxpsiv/id1287152421?mt=8",
      "type": 302
    }, {
      "source": "/",
      "function": "index"
    }, {
      "source": "/index",
      "function": "index"
    }, {
      "source": "/**",
      "function": "index"
    } ]
  }
}
