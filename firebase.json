{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "**",
      "function": "/index"
    }, {
      "source": "ps",
      "destination": "https://itunes.apple.com/jp/app/sdvxpsiv/id1287152421?mt=8",
      "type": 302
    } ]
  }
}
