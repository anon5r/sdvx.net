{
  "functions": {
    "predeploy": [
      "pnpm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "firebase/functions"
  },
  "hosting": {
    "public": "pages/public",
    "trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "redirects" : [ {
      "source": "/apps/ps",
      "destination": "https://itunes.apple.com/jp/app/sdvxpsiv/id1287152421?mt=8",
      "type": 302
    }, {
      "source": "/eacsdvx",
      "destination": "https://p.eagate.573.jp/game/eacsdvx/vi/index.html",
      "type": 302
    }, {
      "source": "/cs",
      "destination": "https://p.eagate.573.jp/game/eacsdvx/vi/index.html",
      "type": 302
    } ],
    "rewrites": [ {
      "source": "/",
      "function": "index"
    }, {
      "source": "/index",
      "function": "index"
    }, {
      "source": "/**",
      "function": "index"
    } ]
  },
  "emulators": {
    "hosting": {
      "host": "localhost",
      "port": "5010"
    }
  }
}